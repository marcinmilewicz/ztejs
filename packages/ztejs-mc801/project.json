{
  "name": "ztejs-mc801",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ztejs-mc801/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/ztejs-mc801",
        "main": "packages/ztejs-mc801/src/index.ts",
        "tsConfig": "packages/ztejs-mc801/tsconfig.lib.json",
        "assets": ["packages/ztejs-mc801/*.md"]
      },
      "dependsOn": ["^build"]
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs ztejs-mc801 {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/ztejs-mc801/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/ztejs-mc801/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
